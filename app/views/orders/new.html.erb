New Order
<form action="<%= orders_url %>" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <label for="order_number">Order Number</label>
    <input type="text" name="order[order_number]" id="order_number">
    <label for="vendor">Vendor Name</label>
    <input type="text" name="order[vendor]" id="vendor">
    <label for="tracking_number">Tracking Number</label>
    <input type="text" name="order[tracking_number]" id="tracking_number">
    <label for="address">Address</label>
    <input type="text" name="order[address]" id="address">
    <input type="submit" value="Place Order">
</form>